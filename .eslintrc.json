{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/react-in-jsx-scope": "off",
        "jsx-a11y/no-noninteractive-element-interactions" : "off",
        "jsx-a11y/click-events-have-key-events" : "off",
        "no-undef" : "off",
        "camelcase" : "off",
        "no-unreachable": "off",
        "react/button-has-type" : "off"
    }
}
